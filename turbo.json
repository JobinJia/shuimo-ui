{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "pre-dev": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "dependsOn": [
        "pre-dev"
      ]
    },
    "dev:doc": {
      "cache": false,
      "dependsOn": [
        "build:postcss",
        "web-component-doc:dev"
      ]
    },
    "web-component-doc:dev": {
      "cache": false,
      "dependsOn": [
        "build:postcss",
        "build:web-component"
      ]
    },
    "build:postcss": {},
    "build:web-component": {},
    "playground": {
      "cache": false,
      "dependsOn": [
        "dev"
      ]
    },
    "clean": {
      "cache": false
    }
  }
}
