[class^='m-'], .m-cursor {
  cursor: url(../assets/cursor/auto.png), auto;

  &:disabled {
    cursor: url(../assets/cursor/disabled.png), auto;
  }
}

.m-cursor-pointer {
  cursor: url(../assets/cursor/pointer.png), auto;
}

.m-cursor-disabled {
  cursor: url(../assets/cursor/disabled.png), auto;
}
